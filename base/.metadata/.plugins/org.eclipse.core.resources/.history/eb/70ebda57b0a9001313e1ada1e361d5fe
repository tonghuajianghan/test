package com.cust.dew.dao.impl;

import org.hibernate.Session;

import com.cust.dew.vo.Administrator;

public class AdministratorDBDao {
  public void saveAdministrator(Administrator a){
	  Session	session = ConnectionFactory.beginSession();
		session.beginTransaction();
		session.save(a);
		session.getTransaction().commit();
	    ConnectionFactory.colseSessionAndSessionFactory();
  }
}
