<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.cust.dew.vo.Commodity" table="commodity">
    <id name="coid" column="coid" type="integer">
      <generator class="increment"/>
    </id>
    <property name="coname" column="coname" type="string"></property>    
    <property name="cotype" column="cotype" type="string"></property> 
    <property name="cotype2" column="cotype2" type="string"></property> 
    <property name="cosssh" column="cosssh" type="string"></property>
    <property name="conewold" column="conewold" type="string"></property>  
    <property name="coreleasetime" column="coreleasetime" type="date"></property>
    <property name="coprise" column="coprise" type="integer"></property>
    <property name="conumber" column="conumber" type="integer"></property>
    <property name="coschool" column="coschool" type="string"></property>
    <property name="codetail" column="codetail" type="string"></property>
    <property name="coimg" column="coimg" type="string"></property>       
  </class>
</hibernate-mapping>