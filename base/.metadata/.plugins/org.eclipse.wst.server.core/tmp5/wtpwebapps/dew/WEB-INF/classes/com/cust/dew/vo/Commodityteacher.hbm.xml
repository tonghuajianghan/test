<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.cust.dew.vo.Commodityteacher" table="jiaoyi">
    <id name="jyid" column="jyid">
     <generator class="increment"></generator>
    </id>
    <many-to-one name="coid"  column="coid" class="com.cust.dew.vo.Commodity" unique="true"></many-to-one>   
    <many-to-one name="teid"  column="teid" class="com.cust.dew.vo.Teacher" unique="true"></many-to-one>   
  </class>
</hibernate-mapping>