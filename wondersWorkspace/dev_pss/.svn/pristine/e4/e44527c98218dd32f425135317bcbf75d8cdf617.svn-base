/* Layout */
body, html { overflow-x:auto; background:#fff; }

.layout-admin { visibility:hidden; _visibility:visible; width:1366px; margin:0 auto; overflow-x:hidden; }
.layout-admin .la-l { width:220px; float:left; background:#293541; }
.layout-admin .la-l .logo { height:80px; background:url(../../images/admin/logo.png) no-repeat top left; }
.layout-admin .la-l .menu { min-height:400px; _height:400px; }
.layout-admin .la-r { width:1144px; float:right; border:1px solid #888; padding-bottom:30px; }
.layout-admin .la-r .nav { height:79px; }
.layout-admin .la-r .banner { height:71px; background:url(../../images/admin/banner-bg.png) no-repeat bottom left; padding-right:40px; }

/* Menu */
.la-l .menu ul { padding-top:70px; }
.la-l .menu li { padding-left:30px; margin-bottom:16px; height:42px; line-height:42px; color:#fff; font-size:16px; font-family:Microsoft Yahei; background:url(../../images/admin/menu-1.png) no-repeat top left; cursor:pointer; }
.la-l .menu li.sel { background-image:url(../../images/admin/menu-2.png); }

/* Nav */
.la-r .nav ul { float:right; margin-top:28px; margin-right:62px; }
.la-r .nav li { float:left; color:#34404c; font-size:14px; font-family:Microsoft Yahei; margin-left:39px; }
.la-r .nav li a { color:#34404c; font-size:14px; font-family:Microsoft Yahei; text-decoration:underline; }

/* Banner */
.la-r .banner dt { margin-top:16px; font-size:16px; font-family:Microsoft Yahei; }
.la-r .banner dt.title { margin-left:24px; float:left; }
.la-r .banner dt.cond { margin-right:10px; float:right; }
.la-r .banner dd.back { float:left; background-image:url(../../images/admin/banner-back.png); }
.la-r .banner dd.back a { width:60px; height:60px; display:block; }
.la-r .banner dd.search { margin-top:17px; margin-right:20px; float:right; background-image:url(../../images/admin/banner-search.png); }
.la-r .banner dd.search a { width:22px; height:22px; display:block; }
.la-r .banner dd.cond { margin-top:12px; margin-right:20px; float:right; }
.la-r .banner dd.cond input { width:150px; height:26px; border:1px #ccc solid; padding:2px 4px; }
.la-r .banner dd.cond select { width:150px; height:32px; border:1px #ccc solid; }

/* Operation */
.la-r .op { height:48px; margin-left:99px; }
.la-r .op a { float:left; margin-top:15px; margin-right:15px; height:23px; line-height:23px; font-size:14px; padding-left:25px; padding-top:2px; font-family:Microsoft Yahei; display:block; background-repeat:no-repeat; }
.la-r .op a.add { background-image:url(../../images/admin/op-add.png); }
.la-r .op a.del { background-image:url(../../images/admin/op-del.png); }
.la-r .op a.off { background-image:url(../../images/admin/op-off.png); }
.la-r .op a.on { background-image:url(../../images/admin/op-on.png); }
.la-r .op a.pwd {  background-image:url(../../images/admin/op-pwd.png); }

/* Center */
.la-r .ct-l { float:left; min-height:500px; _height:500px; width:573px; background:url(../../images/admin/center-bg.png) no-repeat top right; background-position-y:50px; }
.la-r .ct-r { float:right; margin-top:50px; margin-right:60px; }
.la-r .ct-r dl { width:445px; border:1px solid #b1bac2; margin-bottom:20px; clear:both; }
.la-r .ct-r dl dt, .la-r .ct-r dl dd { line-height:46px; height:46px; font-family:Microsoft Yahei; }
.la-r .ct-r dl dt { float:left; font-size:14px; color:#34404c; width:120px; padding-left:5px; }
.la-r .ct-r dl dd { float:right; font-size:16px; color:#000; width:319px; }

/* Bar */
.la-r .btn-bar { margin-left:60px; margin-top:40px; }
.la-r .btn-bar input { margin-right:50px; }

/* Form */
.input, .select, .select-multi, .textarea { border:1px #ccc solid; }
.input { color:#333; height:26px; line-height:26px; font-size:14px; font-style:normal; padding:2px 4px; }
.select { color:#333; height:20px; *height:24px; /* ie7 or older */ line-height:20px; font-size:12px; font-style:normal; }
*+html .select { margin-bottom:-4px; } /* ie7 only */
.select-multi { color:#333; font-size:12px; font-style:normal; }
.textarea { color:#333; padding-left:2px; margin-top:4px; font-size:12px; font-style:normal; resize:none; }
.checkbox { height:18px; height:17px\9; *+height:24px; width:28px; width:24px\9; *+width:27px; }

table td, table th { word-break:break-all; word-wrap:break-word; }

table.list { width:1024px; border-collapse:collapse; table-layout:fixed; margin:0 auto; border:1px solid #ccc; }
table.list th, table.list td { font-size:14px; font-family:Microsoft Yahei; }
table.list th { height:35px; color:#333; background:#b1bac2; border:1px solid #ccc; }
table.list td { height:48px; color:#34404c; padding:2px; border-bottom:1px solid #ccc; }
table.list td a  { color:#34404c; font-size:14px; font-family:Microsoft Yahei; }

table.info { border-collapse:collapse; table-layout:fixed; margin-left:60px; margin-top:35px; }
table.info th, table.info td, table.info td label { height:45px; color:#333; font-size:14px; font-family:Microsoft Yahei; }

/* MERGE */

table.list .page-container ul { padding:6px 0 4px 0; float:right; }
table.list .page-container li { float:left; margin-right:5px; }
table.list .page-container li a, table.list .page-container li label { display:inline; border:1px solid #ccc; background:#f6f7fb; padding:4px 10px; color:#75767d; font-size:14px; font-family:Microsoft Yahei; text-decoration:none; }
table.list .page-container li a:hover { border:1px solid #2d64b3; color:#b60606; }
table.list .page-container li label.current { background:#012e8b; color:#fff; }
table.list .page-container li input { margin:-5px 5px 0 0; width:30px; color:#333; padding-left:2px; height:22px; line-height:22px; font-size:14px; font-style:normal; }table td, table th { word-break:break-all; word-wrap:break-word; }

table.edit { width:100%; *width:auto; border-collapse:collapse; table-layout:fixed; }
table.edit th, table.edit td { height:50px; padding:5px; border-bottom:2px solid #ecedf2; }
table.edit th div {margin-left:35px; float:left;font-size:16px; font-family:Microsoft Yahei; }
table.edit th, table.edit th a { color:#333; font-size:16px; margin-left:30px;font-family:Microsoft Yahei; }
table.edit td, table.edit td a  { color:#75767d; font-size:16px; font-family:Microsoft Yahei; }
table.edit td input.textCont { width:85%; height:26px; border:1px #ccc solid; padding:2px 4px; }
table.edit td input.tagInputCont { width:20%; height:20px; border:1px #ccc solid; }
table.edit td input.tagCont { width:78px; height:27px; border:1px; }
table.edit td .textareaCont { width:85%; border:1px #ccc solid; padding:2px 4px; }
table.edit td select { width:200px; height:26px; border:1px #ccc solid; padding:2px 4px; }

.buttonCont { width:80px; height:26px; border:1px #ccc solid; padding:2px 4px; }