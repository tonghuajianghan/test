package com.wondersgroup.pss.util;

import java.util.UUID;

/**
 * 业务相关工具类
 * 
 * @author computeradd
 */
public class BusinessUtil {
    /**
     * 随机生成32位UUID
     * 
     * @return
     */
    public static String randomUUID() {
        return UUID.randomUUID().toString().replaceAll("-", "");
    }
}
